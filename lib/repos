# A simple shell import that contains the locations of my repos

export ORGMODE_DIR="$HOME/org"
export DOTFILES_DIR="$ORGMODE_DIR/config"

export GTD_DIR="$ORGMODE_DIR/gtd"
export NOTES_PUBLIC_DIR="$ORGMODE_DIR/notes"
export NOTES_PRIVATE_DIR="$ORGMODE_DIR/private"

export PAPERS_DIR="$ORGMODE_DIR/papers"
export BLOG_DIR="$ORGMODE_DIR/blog"
export LEDGER_DIR="$HOME/usr/ledger"
export PASSWORD_STORE_DIR="$HOME/usr/passwords"
